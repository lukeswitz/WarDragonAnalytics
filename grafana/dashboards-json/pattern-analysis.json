      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "timescaledb"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH drone_appearances AS (\n  SELECT \n    drone_id,\n    MIN(time) AS first_seen,\n    MAX(time) AS last_seen,\n    COUNT(DISTINCT time_bucket('5 minutes', time)) AS appearances,\n    ARRAY_AGG(DISTINCT kit_id) AS kits,\n    ARRAY_AGG(DISTINCT COALESCE(rid_make, 'Unknown')) AS makes\n  FROM drones\n  WHERE time >= $__timeFrom()\n    AND time <= $__timeTo()\n    AND ('$kit_filter' = 'All' OR kit_id = '$kit_filter')\n  GROUP BY drone_id\n  HAVING COUNT(DISTINCT time_bucket('5 minutes', time)) > 1\n)\nSELECT \n  drone_id AS \"Drone ID\",\n  appearances AS \"Appearances\",\n  first_seen AS \"First Seen\",\n  last_seen AS \"Last Seen\",\n  EXTRACT(EPOCH FROM (last_seen - first_seen))/60 AS \"Duration (min)\",\n  ARRAY_TO_STRING(kits, ', ') AS \"Kits\",\n  ARRAY_TO_STRING(makes, ', ') AS \"Make\"\nFROM drone_appearances\nORDER BY appearances DESC, last_seen DESC\nLIMIT 50;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Repeated Drone IDs",
      "type": "table"
